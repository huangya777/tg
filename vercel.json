{
  "version": 2,
  "builds": [
    {
      "src": "bot.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/public/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000"
      },
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "bot.py"
    }
  ]
}
